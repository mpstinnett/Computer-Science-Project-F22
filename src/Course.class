public class Course {
    String courseNum;
    String semester;
    Double grade;
    String courseTitle;
    Boolean transfer;

    Course () {};

    String getCourseNum() {
        return courseNum;
    }

    String getSemester() {
        return semester;
    }

    Double getGrade() {
        return grade;
    }

    String getCourseTitle() {
        return courseTitle;
    }

    Boolean getTransfer() {
        return transfer;
    }

}